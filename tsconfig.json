{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src", "src/vite-env.d.ts"],
  "exclude": ["node_modules", "build"]
}
